@page
@model PasswordGenKeyAPP.Pages.UserListModel
@{
}

<div class="text-center">
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in UserListModel.UserList)
            {
<tr>
    <td>@user.Id</td>
    <td>@user.UserName</td>
    <td>@user.Password</td>
</tr>            }
        </tbody>
    </table>
</div>